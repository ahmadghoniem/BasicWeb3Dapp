{
  "name": "web3dapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "hardhat:compile": "cd hardhat && npx hardhat compile",
    "hardhat:test": "cd hardhat && npx hardhat test",
    "hardhat:node": "cd hardhat && npx hardhat node",
    "hardhat:clean": "cd hardhat && npx hardhat clean",
    "hardhat:deploy": "cd hardhat && npx hardhat ignition deploy ignition/modules/SystemModule.ts --network localhost",
    "hardhat:deploy:sepolia": "cd hardhat && npx hardhat ignition deploy ignition/modules/SystemModule.ts --network sepolia --reset",
    "hardhat:verify:sepolia": "cd hardhat && npx hardhat verify --network sepolia",
    "wagmi:generate": "wagmi generate",
    "wagmi:watch": "wagmi generate --watch"
  },
